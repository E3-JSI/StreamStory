{
	"qminer": {
		"dbPath": "/root/db/",
		"path": "../qminer/",
		"mode": "createClean",
		"createPipeline": true,
		"initializeZeros": true
	},
	"models": {
		"SSFName": "/root/models/StreamStory.bin",
		"StreamStoryParams": {
			"transitions": {
				"type": "continuous",
				"timeUnit": "hour"
			},
			"clustering": {
				"type": "dpmeans",
				"lambda": 0.7,
				"minClusts": 10,
				"rndseed": 1,
				"sample": 1,
				"histogramBins": 20
			},
			"pastStates": 2,
			"verbose": true
		}
	},
	"server": {
		"port": 8888,
		"pingInterval": 10000
	},
	"log": {
		"level": "debug",
		"print": {
			"rawData": 10000,
			"streamStory": 100,
			"stores": 10000,
			"broker": 1000,
			"coeff": 100
		}
	},
	"integration": {
		"type": "broker",
		"brokerUrl": "192.168.1.24",
		"zookeperPort": 2181,
		"producerPort": 8090
	},
	"interpolation": "current",
	"replay": false
}
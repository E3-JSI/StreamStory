{
	"qminer": {
		"dbPath": "/root/db/",
		"path": "../qminer/",
		"mode": "createClean",
		"createPipeline": true,
		"initializeZeros": false
	},
	"models": {
		"SSFName": "/root/models/StreamStory.bin",
		"StreamStoryParams": {
			"transitions": {
				"type": "continuous",
				"timeUnit": "hour"
			},
			"clustering": {
				"type": "dpmeans",
				"lambda": 0.7,
				"minClusts": 10,
				"rndseed": 1,
				"sample": 1,
				"histogramBins": 20
			},
			"pastStates": 2,
			"verbose": true
		}
	},
	"server": {
		"port": 8888,
		"pingInterval": 10000
	},
	"log": {
		"level": "debug"
	},
	"integration": {
		"type": "http",
		"brokerUrl": "192.168.1.111",
		"zookeperPort": 2181,
		"producerPort": 8090
	},
	"interpolation": "current",
	"replay": false
}